<h1>My Reviews</h1>
<% @reviews.each do |review| %>
  <div class="reviews" id="review-<%= review.id %>">
    <%= review.title %>
    <%= review.description %>
    <%= review.score %>
    <%= link_to 'Edit Review' %>
    <%= link_to 'Disable Review' %>
    <%= link_to 'Delete Review', profile_order_delete_item_review_path(review.order_item_id, review.id), method: :delete%>
    <% end %>
  </div>
