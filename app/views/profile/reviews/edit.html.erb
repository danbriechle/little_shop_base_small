<h2>Edit Review</h2>

<%= form_for @form_path do |f| %>

<p><%= f.label :title %>
<%= f.text_field :title, required: true %></p>

<p><%= f.label :description %>
<%= f.text_area :description, required: true %></p>

<p><%= f.label :score %>
<%= f.number_field :score, required: true %></p>

<p><%= f.submit %></p>
<% end %>
